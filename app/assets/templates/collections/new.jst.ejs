<% collections.forEach( function(collection) { %>
<a class="collection-add" data-id='<%=collection.escape('id')%>'> <%= collection.escape('title') %> </a>
<br>
<% }) %>
<form class="new-collection">
  <input type="text" name="title"></input>
  <button class="submit-new-col">Save to new collection</button>
</form>
